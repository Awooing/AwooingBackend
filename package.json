{
  "name": "@awooing/backend",
  "version": "2.0.0",
  "description": "Backend for the Awooing.moe website",
  "main": "build/index.js",
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "compile:watch": "yarn compile --watch",
    "start:dev": "NODE_ENV=development tsnd src/index.ts",
    "start:prod": "NODE_ENV=production yarn compile && node build/index.js",
    "style:format": "prettier --write src/**/*.ts --list-different",
    "style:lint": "eslint",
    "postinstall": "husky install"
  },
  "author": {
    "name": "Project Awooing",
    "url": "https://github.com/Awooing",
    "email": "awooing@awooing.moe"
  },
  "contributors": [
    {
      "name": "Niputi",
      "url": "https://github.com/Niputi_"
    },
    {
      "name": "Mia / VottusCode",
      "url": "https://github.com/VottusCode"
    }
  ],
  "keywords": [
    "awoo",
    "awooing.moe"
  ],
  "private": true,
  "license": "PROPRIETARY",
  "dependencies": {
    "@fasteerjs/fasteer": "^1.0.1",
    "@typegoose/typegoose": "^7.4.5",
    "argon2": "^0.26.2",
    "aws-sdk": "^2.706.0",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "fastify": "^3.9.1",
    "fastify-cors": "^5.0.0",
    "fastify-rate-limit": "^4.1.0",
    "husky": "^5.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.20",
    "shortid": "^2.2.16",
    "slugify": "^1.4.4",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.28",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1"
  }
}
